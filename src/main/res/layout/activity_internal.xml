<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
				xmlns:tools="http://schemas.android.com/tools"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:paddingLeft="@dimen/activity_horizontal_margin"
				android:paddingRight="@dimen/activity_horizontal_margin"
				android:paddingTop="@dimen/activity_vertical_margin"
				android:paddingBottom="@dimen/activity_vertical_margin"
				android:gravity="center_vertical"
				tools:context="com.brandonmkelley.parse_app.InternalActivity">

	<TextView
		android:id="@+id/internal_title"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="@string/internal_title"
		android:textColor="@color/black"
		android:textSize="30sp"
		android:layout_centerHorizontal="true"/>

	<Space
		android:id="@+id/internal_space_between"
		android:layout_below="@id/internal_title"
		android:layout_width="wrap_content"
		android:layout_height="30dp"/>

	<ListView
		android:id="@+id/internal_list"
		android:layout_below="@id/internal_space_between"
		android:layout_width="250dp"
		android:layout_height="wrap_content"
		android:layout_centerHorizontal="true"
		android:gravity="center_horizontal"/>

	<Space
		android:id="@+id/internal_space_logout"
		android:layout_below="@id/internal_list"
		android:layout_width="wrap_content"
		android:layout_height="20dp"/>

	<Button
		android:id="@+id/internal_logout"
		android:layout_below="@id/internal_space_logout"
		android:layout_width="100dp"
		android:layout_height="wrap_content"
		android:layout_centerHorizontal="true"
		android:text="@string/internal_logout"/>

	<Space
		android:id="@+id/internal_space_bottom"
		android:layout_below="@id/internal_logout"
		android:layout_width="wrap_content"
		android:layout_height="150dp"/>

</RelativeLayout>
